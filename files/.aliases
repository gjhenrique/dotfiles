alias zshsource="source ~/.zprofile && source ~/.zshrc"

alias xc="wl-copy"
alias xco="wl-paste"
alias pg="ping google.com"

alias be="bundle exec"
alias bs="bundle exec spring"

alias em="emacsclient -ct"
